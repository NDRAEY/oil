type Flower(water_amount: int) {
    water_amount = water_amount
}

enum Pot {
    Full(flower: Flower),
    Empty
}

fn main() {
    let pot = Pot.Full(Flower(15))
    let amount: int = -1
    match pot {
        Pot.Full { flower } -> {
            amount = flower.water_amount
        }
        Empty -> {
            amount = 0
        }
    }
}
