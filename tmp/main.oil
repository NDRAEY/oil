type Flower(water_level: int) {
    pub let water_level: int = water_level
    pub fn water(add: int) {
        self.water_level += add
    }
}

enum Pot {
    Full(flower: Flower),
    Nothing
}

fn main() {
    let flower1 = Pot.Nothing()
    let flower2 = Pot.Full(Flower(15))
    let result = Flower(15).water(3)
}
