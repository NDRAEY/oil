use std/option for Option

type Flower(water_level: int) {
    pub let water_level: int = water_level
    pub fn water(add: int) {
        self.water_level += add
    }
}

enum Pot {
    Full(flower: Flower),
    Nothing
}

fn main() {
    let pot = Option.Some(Pot.Full(Flower(15)))
    match pot {
        Option.Some { element } -> {
            let int: int = element
            if int == 14 {
                let a = int
            } elif int == 15 {
                let b = int
            } else {
                let c = int
            }
        }
        Option.None() -> {
        }
    }
}
